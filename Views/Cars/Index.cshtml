@model IEnumerable<WebApplication.Models.Car>

@Styles.Render("~/Content/css")
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h2> Car Admin Space </h2>
    <center>
        @Html.ActionLink("Create New", "Create", new object { }, new { @class = "btn btn-info" })
    </center>

    <br />
    <table class="table table-bordered">
        <tr>
           
            <th>
                @Html.DisplayNameFor(model => model.categories)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.brand)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.models)
            </th>


            <th>
                @Html.DisplayNameFor(model => model.price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.reserve)
            </th>
            <th></th>
        </tr>



        @foreach (var item in Model)
        {



            <tr>
                
                <td>
                    @Html.DisplayFor(modelItem => item.categories)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.brand)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.models)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.reserve)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { Rnum = item.Rnum }, new { @class = "btn btn-warning" }) |
                    @Html.ActionLink("Details", "Details", new { num = item.Rnum }, new { @class = "btn btn-success" }) |
                    @Html.ActionLink("Delete", "Delete", new { num = item.Rnum }, new { @class = "btn btn-danger" })
                </td>
            </tr>
        }

    </table>
    <br />
          <center>

              @Html.ActionLink("Reservations page", "index","resrvations", new object { }, new { @class = "btn btn-warning" })
              @Html.ActionLink("Account admin", "Index", "Accounts", new object { }, new { @class = "btn btn-info" })
              @Html.ActionLink("Go home page", "Index", "Home", new object { }, new { @class = "btn btn-danger" })
          </center>


</body>
</html>
